{
  "name": "test-marked",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.index.ts",
  "scripts": {
    "build": "npm-run-all --serial delete:file build:server:dev build:client:dev addtag move:file",
    "push": "clasp push",
    "delete:file": "rimraf -g dist/*.{js,html}",
    "move:file": "cpx \"src/**/*.html\" dist",
    "build:server:dev": "webpack --mode=development --config webpack.config.server.js",
    "build:server:prod": "webpack --mode=production --config webpack.config.server.js --node-env=production",
    "build:client:dev": "webpack --mode=development --config webpack.config.client.js",
    "build:client:prod": "webpack --mode=production --config webpack.config.client.js --node-env=production",
    "addtag": "node client.addtag.js",
    "watch": "webpack --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@google/clasp": "^2.4.1",
    "@monaco-editor/react": "^4.4.2",
    "@types/google-apps-script": "^1.0.46",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.4",
    "babel-polyfill": "^6.26.0",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.1",
    "gas-webpack-plugin": "^2.2.1",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "html-webpack-plugin": "^5.5.0",
    "marked": "^4.0.14",
    "mermaid": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-toastify": "^8.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
